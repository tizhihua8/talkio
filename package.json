    "expo-brightness": "~12.0.0",
    "expo-calendar": "~13.0.0",
    "expo-clipboard": "~7.0.0",
    "expo-haptics": "~14.0.0",
    "expo-image": "~2.0.0",
    "expo-linking": "~7.0.0",
